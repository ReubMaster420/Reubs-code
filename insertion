def insertion(list):
    for i in range(1, len(list)):

        currentvalue = list[i]
        j = i - 1
        print(list)
        while j >= 0 and currentvalue < list[j]:
            list[j + 1] = list[j]
            j -= 1
        list[j + 1] = currentvalue

list = [12 , 48, 3 , 2 , 1 , 16]
insertion(list)
